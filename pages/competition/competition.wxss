/* pages/competition/competition.wxss */
page {
  background-image: url(https://wx3.sinaimg.cn/mw690/006cV2kkgy1gcz9iabu19j30yi22oabf.jpg);
  background-size: cover;
}

.lb{
  width: 100%;
  height:400rpx;
}

.imageLb{
  width:100%;
  height:400rpx;
}

.border{
  background-color: #310f1b;
  color:cyan;
  font-family: Arial, Helvetica, sans-serif;
  text-align: center;
  text-emphasis-style: solid;
}

.box {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  flex-wrap: wrap;
  margin: 5% auto 10% ;
}

.boxitem {
  width: 180rpx;
  height: 180rpx;
  margin: 25rpx;
  background-color: white;
  border-radius: 25rpx;
  display: flex;
  flex-direction: column;
}

.boxitem_top {
  width: 70%;
  height: 70%;
  margin: 0 auto;
  display: flex;
  justify-content: center;
  align-items: center;
}

.boxitem_top image {
  width: 80%;
  height: 80%;
}

.boxitem_bottom {
  font-weight: 400;
  text-align: center;
}

.find{
  flex-direction: column;
  color:#4b1e2f;
  font-size: 20px;
  font-style: oblique;
  font-family: 'Times New Roman', Times, serif;
}

.findPartner{
  float: right;
  width: 180rpx;
  height: 180rpx;
  margin: 25rpx;
  background-color: white;
  border-radius: 25rpx;
  display: flex;
  flex-direction: column;
}
